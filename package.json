{
  "name": "webpack-init",
  "version": "1.0.0",
  "description": "just webpack init project...",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot  --config ./build/webpack.dev.conf.js",
    "dev": "webpack-dev-server --inline --hot --config ./build/webpack.dev.conf.js",
    "dev-info": "webpack-dev-server --inline --hot --progress --profile --display-chunks --display-modules --display-error-details --config ./build/webpack.dev.conf.js",
    "test": "webpack-dev-server --inline --hot --config ./build/webpack.test.conf.js",
    "build": "rimraf dist && webpack --progress --profile --display-chunks --display-modules --display-error-details --config ./build/webpack.pro.conf.js",
    "dev-build": "rimraf dist && webpack --progress --display-error-details --config ./build/webpack.dev.conf.js",
    "dev-build-details": "rimraf dist && webpack --progress --profile --display-chunks --display-modules --display-error-details --config ./build/webpack.dev.conf.js",
    "pro-build": "rimraf dist && webpack --progress --hide-modules --config ./build/webpack.pro.conf.js",
    "deploy": "node build/build.js"
  },
  "keywords": [
    "webpack",
    "es6"
  ],
  "author": "cycold@qq.com",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-preset-stage-2": "^6.13.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "inject-loader": "^3.0.0-beta1",
    "ip": "^1.1.3",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.10.0",
    "ora": "^0.3.0",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "shell-arguments": "^1.1.9",
    "shelljs": "^0.7.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-dev-server": "^1.16.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "jquery": "^3.1.0",
    "lodash": "^4.16.1"
  }
}
